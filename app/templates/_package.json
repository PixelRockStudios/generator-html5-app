{
	"name": "<%= _.slugify(appname) %>",
	"description": "<%= description %>",
	"includeModernizr": "<%= includeModernizr %>",
	"includejQuery": "<%= includejQuery %>",
	"author":  {
		"name": "<%= authorName %>",
		"email": "<%= authorEmail %>",
		"url": "<%= authorURL %>"
	},
	"licenses": [
		{
			"type": "<%= license %>"
		}
	],
	"repository": "",
	"readme": "<%= description %>",
	"version": "1.0.0",
	"dependencies": {},
	"devDependencies": {
		"grunt": "*",
		"grunt-contrib-copy": "*",
		"grunt-contrib-concat": "*",<% if (coffee) { %>
		"grunt-contrib-coffee": "*",<% } %>
		"grunt-contrib-uglify": "*",<% if (includeCompass) { %>
		"grunt-contrib-compass": "*",<% } %>
		"grunt-contrib-jshint": "*",
		"grunt-contrib-cssmin": "*",
		"grunt-contrib-connect": "*",
		"grunt-contrib-clean": "*",
		"grunt-contrib-htmlmin": "*",
		"grunt-bower-install": "*",
		"grunt-contrib-imagemin": "*",
		"grunt-contrib-watch": "*",<% if (testFramework === "jasmine") { %>
		"grunt-contrib-jasmine": "*",<% } %>
		"grunt-rev": "*",
		"grunt-autoprefixer": "*",
		"grunt-usemin": "*",<% if (testFramework === "mocha") {  %>
		"grunt-mocha": "*",<% } %><% if (includeModernizr) { %>
		"grunt-modernizr": "*",<% } %>
		"grunt-newer": "*",
		"grunt-svgmin": "*",
		"grunt-concurrent": "*",
		"load-grunt-tasks": "*",
		"time-grunt": "*",
		"jshint-stylish": "*"
	},
	"engines": {
		"node": ">=0.8.0"
	}
}